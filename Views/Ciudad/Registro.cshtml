@model Ciudad

<h1>Registrar Ciudad</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="NombreC">Ciudad</label>
        <input asp-for="NombreC" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="Lider">Lider de Gimnasio</label>
        <input asp-for="Lider" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="Foto">Foto</label>
        <input asp-for="Foto" class="form-control">
    </div>
    <button class="btn btn-primary">Registrar</button>
</form>